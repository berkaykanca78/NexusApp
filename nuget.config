<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!-- nuget.org proxy (Nexus üzerinden) -->
    <add key="nexus" value="http://localhost:8081/repository/nuget.org-proxy/index.json" protocolVersion="3" allowInsecureConnections="true" />
    <!-- Nexus hosted (kendi paketleriniz) - repo oluşturduktan sonra yorumu kaldırın -->
    <!-- <add key="nexus-hosted" value="http://localhost:8081/repository/nuget-hosted/index.json" protocolVersion="3" allowInsecureConnections="true" /> -->
  </packageSources>
  <packageSourceMapping>
    <packageSource key="nexus">
      <package pattern="*" />
    </packageSource>
    <!-- <packageSource key="nexus-hosted">
      <package pattern="*" />
    </packageSource> -->
  </packageSourceMapping>
</configuration>
